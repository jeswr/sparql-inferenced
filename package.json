{
  "name": "sparql-inferenced",
  "version": "1.0.0",
  "description": "An RDF inferencer that extends the HyLAR (https://github.com/ucbl/HyLAR-Reasoner.git) inferencing engine to allow inferencing with SPARQL Construct queries",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/sparql-inferenced.git"
  },
  "keywords": [
    "SPARQL",
    "HyLAR",
    "RDF",
    "inference"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/sparql-inferenced/issues"
  },
  "homepage": "https://github.com/jeswr/sparql-inferenced#readme",
  "dependencies": {
    "rdf-validate-shacl": "^0.2.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "rdf-js": "^1.0.2",
    "ts-jest": "^26.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ]
}
